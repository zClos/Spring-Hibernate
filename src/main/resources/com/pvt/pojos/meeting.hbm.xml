<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pvt.pojos.Meeting" table="meeting">
        <id name="meetingId" column="meeting_id">
            <generator class="identity"/>
        </id>
        <property name="subject" column="subject" type="java.lang.String"/>
        <set name="employees" table="employee_meeting" lazy="true" fetch="select" inverse="true">
            <key column="meeting_id"/>
            <many-to-many column="employee_id" class="com.pvt.pojos.Employee"/>
        </set>
    </class>
</hibernate-mapping>